Q1
import random

n=int(input('Enter length of tuple: '))

nums=()
for i in range(n):
    a=random.randint(0,100)
    nums+=(a,)

print(nums)

new_nums=()

for i in range(0,n,2):
    new_nums+=(nums[i],)
    
print(new_nums)

